prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: libMems 
Description: c++ library supporting DNA sequence and genome alignment 
Version: @VERSION@
Requires: libGenome-1.3 libMUSCLE-3.7
Libs: -L${libdir} @OPENMP_CXXFLAGS@ -lMems-@GENERIC_API_VERSION@ @BOOST_LDFLAGS@ @BOOST_SYSTEM_LIB@ @BOOST_FILESYSTEM_LIB@ @BOOST_PROGRAM_OPTIONS_LIB@ @BOOST_IOSTREAMS_LIB@ @LIBS@ 
Cflags: -DBOOST_FILESYSTEM_VERSION=2 -I${includedir}/@GENERIC_LIBRARY_NAME@-@GENERIC_API_VERSION@ @BOOST_CPPFLAGS@ @OPENMP_CXXFLAGS@ @EXTRA_CXX_FLAGS@ 

